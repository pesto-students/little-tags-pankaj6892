ntity = () => {\r\n    if (itemQuantity < 30) {\r\n      setItemQuantity(itemQuantity + 1);\r\n    } else {\r\n      alert('You cannot order more than 30 quantity of a product');\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className='row cart-item'>\r\n      <div className='col-sm-3'>\r\n        <div className='product-img-cart'>\r\n          <img\r\n            src={props.productImage}\r\n            alt={props.productName}\r\n            height='150px'\r\n          />\r\n        </div>\r\n      </div>\r\n      <div className='col-sm-6'>\r\n        <div className='mg-20'>\r\n          <h3>{props.productName}</h3>\r\n          <span>Quantity</span>\r\n          <button\r\n            className='increment-decrement ml-3'\r\n            onClick={() => {\r\n              updateItem(props.productId, itemQuantity, 'decrease');\r\n            }}\r\n          >\r\n            -\r\n          </button>\r\n          <span className='item-count'>{itemQuantity}</span>\r\n          <button\r\n            className='increment-decrement'\r\n            onClick={() => {\r\n              updateItem(props.productId, itemQuantity, 'increase');\r\n            }}\r\n          >\r\n            +\r\n          </button>\r\n          <div className='pt-4'>\r\n            <button\r\n              className='remove-from-cart'\r\n              onClick={() => {\r\n                removeItem(props.productId);\r\n              }}\r\n            >\r\n              Remove from Cart\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div className='col-sm-3'>\r\n        <div className='mg-20'>\r\n          <h3>â‚¹ {totalPrice}</h3>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CartItem;\r\n","H:\\Pesto\\Litttle-tags Project\\Code\\final_project\\little-tags-pankaj6892\\src\\pages\\AddressPage\\AddressPage.jsx",[],"H:\\Pesto\\Litttle-tags Project\\Code\\final_project\\little-tags-pankaj6892\\src\\components\\Address\\Addres